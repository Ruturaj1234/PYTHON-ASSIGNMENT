marks= int(input ("enter the marks from 0 to 100"))
if marks >= 90:
    print(" your Grade is A+")
elif marks>= 80:
    print("your grade is A")
elif marks>= 70:
    print("your grade is B")
elif marks>= 60:
    print("your grade is C")
elif marks>= 50:
    print("your grade is D")
else:
    print("FAIL")
